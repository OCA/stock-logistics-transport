<odoo>

    <record id="fleet_vehicle_model_category_view_tree" model="ir.ui.view">
        <field
            name="name"
        >fleet.vehicle.model.category.tree.inherit.tms.account.asset</field>
        <field name="model">fleet.vehicle.model.category</field>
        <field name="inherit_id" ref="fleet.fleet_vehicle_model_category_view_tree" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='name']" position="after">
                <field name="asset_profile_id" />
            </xpath>
        </field>
    </record>

    <record id="fleet_vehicle_model_category_view_form" model="ir.ui.view">
        <field
            name="name"
        >fleet.vehicle.model.category.form.inherit.tms.account.asset</field>
        <field name="model">fleet.vehicle.model.category</field>
        <field name="inherit_id" ref="fleet.fleet_vehicle_model_category_view_form" />
        <field name="arch" type="xml">
            <xpath expr="//group[1]" position="after">
                <group name="tms" string="Asset Profile">
                    <field name="asset_profile_id" />
                </group>
            </xpath>
        </field>
    </record>
</odoo>
