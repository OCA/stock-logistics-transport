<odoo>
    <record id="res_config_settings_view_form" model="ir.ui.view">
        <field name="name">res.config.settings.view.form.inherit.tms</field>
        <field name="model">res.config.settings</field>
        <field name="priority" eval="40" />
        <field name="inherit_id" ref="account.res_config_settings_view_form" />
        <field name="arch" type="xml">
            <xpath expr="//block[@name='trips_settings_container']" position="inside">
                <setting
                    help="Manage analytic account for trips or routes"
                    invisible="not module_tms_account"
                >
                    <field name="group_analytic_accounting" />
                </setting>
            </xpath>
        </field>
    </record>
</odoo>
