<?xml version="1.0" ?>
<openerp>

    <data noupdate="0">

<record id="email_template_tracking" model="email.template">
            <field name="name">Shipping notification</field>
            <field name="email_from">${(object.user_id.email or object.company_id.email or 'noreply@localhost')|safe}</field>
            <field name="subject">Shipping notification</field>
            <field name="partner_to">${object.partner_id.id}</field>
            <field name="model_id" ref="stock.model_stock_picking"/>
            <field name="auto_delete" eval="True"/>
            <field name="lang">${object.partner_id.lang}</field>
            <field name="body_html"><![CDATA[
<head>

  </head>
  <body>
      <tbody>
        <tr>
          <td align="left">Dear ${object.partner_id.name}</strong>,</td><br/>
        </tr>

        <tr>
          <td align="left">your order ${object.name} has been shipped today.</td>
        </tr>

      </tbody>
    </table>
  </body>
	]]></field>
        </record>
    </data>
</openerp>
